    "@unocss/preset-icons": "0.44.7",
    "@vitejs/plugin-vue": "3.0.1",
    "autoprefixer": "10.4.7",
    "capacitor-set-version": "2.0.3",
    "cordova-res": "0.15.4",
    "eslint": "8.20.0",
    "lint-staged": "13.0.3",
