{
  "name": "app-nestjs",
  "version": "0.0.0",
  "private": true,
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "rm -rf dist && nest start --watch",
    "android:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"./**/*.{ts,vue}\"",
    "lint:fix": "eslint --fix \"./**/*.{ts,vue}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "app:create": "ts-node -r tsconfig-paths/register --project ./tsconfig.json ./scripts/index.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "migrate:init": "mikro-orm migration:create --initial",
    "migrate:create": "npx mikro-orm migration:create",
    "migrate:update": "npx mikro-orm schema:update --run",
    "migrate:up": "npx mikro-orm migration:up",
    "migrate:down": "npx mikro-orm migration:down",
    "migrate:fresh": "npx mikro-orm schema:fresh --run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.135.0",
    "@casl/ability": "5.4.4",
    "@liaoliaots/nestjs-redis": "9.0.1",
    "@mikro-orm/cli": "5.2.3",
    "@mikro-orm/core": "5.2.3",
    "@mikro-orm/migrations": "5.2.3",
    "@mikro-orm/nestjs": "5.1.0",
    "@mikro-orm/postgresql": "5.2.3",
    "@mikro-orm/reflection": "5.2.3",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/apollo": "10.0.19",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.0.5",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.5",
    "@nestjs/graphql": "10.0.21",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.5",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-express": "3.10.0",
    "apollo-server-plugin-response-cache": "3.7.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "csv-parse": "5.3.0",
    "date-fns": "2.29.1",
    "dotenv": "16.0.1",
    "firebase-admin": "11.0.0",
    "graphql": "16.5.0",
    "graphql-relay": "0.10.0",
    "graphql-scalars": "1.17.0",
    "graphql-type-json": "0.3.2",
    "handlebars": "4.7.7",
    "ioredis": "5.2.2",
    "joi": "17.6.0",
    "jose": "4.8.3",
    "lodash": "4.17.21",
    "ms": "3.0.0-canary.1",
    "nanoid": "3.3.4",
    "nestjs-i18n": "9.1.5",
    "nodemailer": "6.7.7",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "request-ip": "3.3.0",
    "resolve-accept-language": "1.1.12",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "typescript": "4.7.4",
    "ua-parser-js": "1.0.2",
    "utility-types": "3.10.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@faker-js/faker": "7.3.0",
    "@mikro-orm/seeder": "5.2.3",
    "@nestjs/cli": "8.2.6",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.5",
    "@oku/config": "workspace:*",
    "@oku/tsconfig": "workspace:*",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/graphql-upload": "8.0.11",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.35",
    "@types/nodemailer": "6.4.4",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/request-ip": "0.0.37",
    "@types/supertest": "2.0.12",
    "@types/ua-parser-js": "0.7.36",
    "@types/uuid": "8.3.4",
    "eslint": "8.20.0",
    "google-auth-library": "8.1.1",
    "jest": "28.1.3",
    "json-to-ts": "1.7.0",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/src/mikro-orm.config.js"
    ]
  }
}
